<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :url %>
    <%= f.input :description %>
    <%= f.input :ord %>
    <%= f.input :display %>
  </div>

  <div class="form-actions">
    <%= f.button :submit(@project) %>
  </div>
<% end %>
